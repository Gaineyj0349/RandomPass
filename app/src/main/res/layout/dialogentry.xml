<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:theme="@style/Theme.AppCompat.CompactMenu"
    android:gravity="center_horizontal">
    <TextView
        android:id="@+id/passIDdialog"
        android:paddingTop="4dp"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textSize="25sp"
        android:text="dslfkn24lksd"
        android:textColor="#ffffff"
        android:background="@color/colorAccent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />
    <View
        android:paddingTop="4dp"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/colorPrimaryDark"

        />
    <TextView
        android:paddingTop="4dp"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textSize="25sp"
        android:text="@string/selecticon"
        android:textColor="#000000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />

    <LinearLayout
        android:paddingTop="4dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
      <ImageButton
            android:tint="@color/colorPrimaryDark"
         
         android:id="@+id/ib1"
         android:src="@drawable/business"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content" />
         <ImageButton

            android:tint="@color/colorPrimaryDark"
            android:id="@+id/ib2"
            android:src="@drawable/grocery"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
         <ImageButton
            android:tint="@color/colorPrimaryDark"
            
            android:id="@+id/ib3"
            android:src="@drawable/ic_audiotrack_black_24dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
         <ImageButton
            android:tint="@color/colorPrimaryDark"
            
            android:id="@+id/ib4"
            android:src="@drawable/ic_build_black_24dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />


    </LinearLayout>

    <LinearLayout
        android:paddingTop="4dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <ImageButton
            android:tint="@color/colorPrimaryDark"

            android:id="@+id/ib5"
            android:src="@drawable/ic_credit_card_black_24dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageButton
            android:tint="@color/colorPrimaryDark"

            android:id="@+id/ib6"
            android:src="@drawable/ic_devices_other_black_24dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <ImageButton
            android:tint="@color/colorPrimaryDark"

            android:id="@+id/ib8"
            android:src="@drawable/ic_event_seat_black_24dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageButton
            android:tint="@color/colorPrimaryDark"

            android:id="@+id/ib9"
            android:src="@drawable/ic_face_black_24dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>



    <LinearLayout
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
         <ImageButton
            android:tint="@color/colorPrimaryDark"
            
            android:id="@+id/ib7"
            android:src="@drawable/ic_palette_black_24dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

         <ImageButton
            android:tint="@color/colorPrimaryDark"
            
            android:id="@+id/ib10"
            android:src="@drawable/ic_person_outline_black_24dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
         <ImageButton
            android:tint="@color/colorPrimaryDark"
          
            android:id="@+id/ib11"
            android:src="@drawable/ic_spa_black_24dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
         <ImageButton
            android:tint="@color/colorPrimaryDark"
            
            android:id="@+id/ib12"
            android:src="@drawable/transportation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </LinearLayout>

    <View
        android:paddingTop="4dp"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/colorPrimaryDark"
        />



    <EditText
        android:hint="Enter a label for this password"
        android:id="@+id/etInDialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:textAlignment="center"


   />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center_vertical">
        <Button
            android:id="@+id/buttonInDialog2"
            style="@style/Widget.AppCompat.Button.Colored"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Cancel"
         />

    <Button
       android:id="@+id/buttonInDialog"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:text="Save To Vault"
   />
    </LinearLayout>

</LinearLayout>
